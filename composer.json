{
  "name": "bryangruneberg/laragoon",
  "description": "The Laragoon utility package",
  "keywords": ["laravel","console","devops","deploy","amazee","amazee.io","lagoon","docker"],
  "homepage": "https://laragoon.io",
  "license": "MIT",
  "authors": [
    { 
      "name": "Bryan Gruneberg", 
      "email": "hello@bryangruneberg.com"
    },
    { 
      "name": "Blaize Kaye", 
      "email": "blaize.kaye@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.6.4",
    "amazee/laragoon-support-libs": "^0.1"
  },
  "autoload": {
    "psr-4": {
      "Bryangruneberg\\Laragoon\\": "src/"
    }
  },
  "extra": {
    "laravel": {
        "providers": [
            "Bryangruneberg\\Laragoon\\LaragoonServiceProvider"
        ],
        "aliases": {
            "Laragoon": "Bryangruneberg\\Laragoon\\LaragoonFacade"
        }
    }
  }
}
